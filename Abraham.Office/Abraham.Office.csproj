<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageReadmeFile>Readme.md</PackageReadmeFile>
    <Copyright>Oliver Abraham</Copyright>
    <Description>Generator for docx files, i.e. to generate docx invoices from a docx template.</Description>
    <PackageProjectUrl>https://github.com/OliverAbraham/Abraham.Office</PackageProjectUrl>
    <RepositoryUrl>https://github.com/OliverAbraham/Abraham.Office</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseFile>C:\GIT\Libs\WPFWindowLayoutManager\WPFWindowLayoutManager\license-gpl-3.0.md</PackageLicenseFile>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Readme.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
